	<queryString>
		<![CDATA[SELECT CONCAT(P.Nombres, ' ', P.Apellidos) AS Paciente, convert(varchar(10), C.Fecha, 103) AS Fecha, C.TipoConsulta AS Tipo, CONCAT(E.Nombres, ' ', E.Apellidos) AS Doctor,
	T.Descripcion AS Tratamiento, T.Precio as Precio
FROM Consultas C, Empleados E, Pacientes P, Consultas_Tratamientos CT, Tratamientos T
WHERE C.Expediente = P.Expediente AND C.IDEmpleadoDoctor = E.IDEmpleado AND CT.IDConsulta = C.IDConsulta AND CT.IDTratamiento = T.IDTratamiento
	AND C.Expediente = $P{Paciente}
ORDER BY C.Fecha DESC;]]>
	</queryString>
