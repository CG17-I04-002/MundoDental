	<queryString>
		<![CDATA[SELECT L.Local AS Clinica,P.IDProducto AS ID, P.Nombre AS Producto, C.Categoria AS Categoria,
	SUM(CASE WHEN O.Transaccion = 'Compra'THEN OP.Cantidad ELSE -OP.Cantidad END) AS Existencias
FROM Operaciones O, Operaciones_Detalles OP, Productos P, Locales L, Categorias C
WHERE  O.IDOperacion = OP.IDOperacion AND OP.IDProducto = P.IDProducto AND O.IDLocal = L.IDLocal
	AND O.IDLocal = $P{Local} AND O.Fecha BETWEEN $P{FechaIni} AND $P{FechaFin}
GROUP BY P.IDProducto, P.Nombre, C.Categoria, L.Local;]]>
	</queryString>
