	<queryString>
		<![CDATA[SELECT convert(varchar(10), C.Fecha, 103) AS Fecha, CONCAT(P.Nombres, ' ', P.Apellidos) AS Paciente, CONCAT(E.Nombres, ' ', E.Apellidos) AS Doctor,  C.Costo as Costo, C.Descuento as Descuento, C.Total as Total, L.Local AS Clinica, C.TipoConsulta AS Tipo, C.Estado AS Estado 
FROM Consultas C, Empleados E, Locales L, Pacientes P
WHERE C.IDEmpleadoDoctor = E.IDEmpleado AND C.IDLocal = L.IDLocal AND C.Expediente = P.Expediente 
	AND C.IDLocal = $P{Local} AND C.TipoConsulta = $P{tipoConsulta} AND C.Fecha BETWEEN $P{FechaIni} AND $P{FechaFin};]]>
	</queryString>
