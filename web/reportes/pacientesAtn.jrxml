	<queryString>
		<![CDATA[SELECT COUNT(C.expediente)  as Consultas, CONCAT(P.Nombres, ' ', P.Apellidos) AS Paciente,  L.local AS Local
FROM Consultas C, Locales L, Pacientes P
WHERE C.expediente = P.expediente AND C.idLocal = L.idLocal 
	AND C.IDLocal = $P{Local} AND C.Fecha BETWEEN $P{FechaIni} AND $P{FechaFin}
GROUP BY P.Nombres, P.Apellidos, L.Local
ORDER BY COUNT(C.expediente) DESC;]]>
	</queryString>
